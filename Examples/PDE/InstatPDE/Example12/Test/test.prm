# Listing of Parameters for PDE Instat Example 12
# --------------------------------------------------------------
subsection main parameters
  set prerefine = 0
  set local_prerefine = 1
  set num_intervals = 5
  set interpolate_initial = true
end

subsection Local PDE parameters
 	   set constant_k	= 1.e-8
	   set G_c		= 1.0
	   set Young_modulus  = 1.0
	   set Poisson_ratio  = 0.2
 	   set sigma			= 1.0 # new
end

subsection newtonsolver parameters
  # maximal number of linesearch steps
  # For the modified instat_step Newton, do not set this number too high
  set line_maxiter         = 4

  # reduction rate for the linesearch damping paramete
  set linesearch_rho       = 0.1

  # global tolerance for the newton iteration
  set nonlinear_global_tol = 1.e-10

  # maximal number of newton iterations
  set nonlinear_maxiter    = 1000

  # minimal  newton reduction, if actual reduction is less, matrix is rebuild
  # the smaller this value, the more often we rebuild the matrix
  set nonlinear_rho        = 0.9

  # relative tolerance for the newton iteration
  set nonlinear_tol        = 1.e-6
end

subsection output parameters
  # File format for the output of solution variables
  set file_format       = .vtk

  # Iteration Counters that should not reflect in the outputname, seperated by
  # `;`
  set ignore_iterations = PDENewton;Cg
  
  # Name of the logfile
  set logfile           = dope.log

  # Do not write files whose name contains a substring given here by a list of
  # `;` separated words
  set never_write_list  = Gradient;Residual;Hessian;Tangent;Adjoint;Update;LastTime;State
      
  # Defines what strings should be printed, the higher the number the more
  # output
  set printlevel        = 6
  
  # Set the precision of the newton output
  set number_precision	 = 2

  # Set manually the machine tolarance for the output
  set eps_machine_set_by_user	 = 1.0e-11


  # Directory where the output goes to
  set results_dir       = ./

end


